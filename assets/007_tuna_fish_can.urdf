<?xml version="1.0"?>
<robot name="007_tuna_fish_can">
  <link name="object">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="007_tuna_fish_can_berkeley_meshes/007_tuna_fish_can/poisson/textured.obj" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="tuna_can_material">
        <color rgba="0.8 0.7 0.6 1.0"/>
      </material>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="007_tuna_fish_can_berkeley_meshes/007_tuna_fish_can/poisson/nontextured.stl" scale="1.0 1.0 1.0"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0.015" rpy="0 0 0"/>
      <mass value="0.12" />
      <!-- Inertia tensor for a typical tuna can (cylinder): -->
      <!-- Estimated dimensions: diameter ~8.5cm, height ~3cm -->
      <!-- Using cylinder inertia: Ixx = Iyy = (1/12)*m*(3*r^2 + h^2), Izz = (1/2)*m*r^2 -->
      <inertia 
        ixx="0.0002" ixy="0.0" ixz="0.0" 
        iyy="0.0002" iyz="0.0" 
        izz="0.0003"/>
    </inertial>
  </link>
</robot>